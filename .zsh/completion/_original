#autoload

local ret=1

_complete && ret=0

if [[ -n "$IPREFIX$PREFIX$SUFFIX$ISUFFIX" && ${compstate[nmatches]} -gt 1 ]]; then

    local expl

    compstate[pattern_match]='*'

    _description -V original expl original

    compadd "$expl[@]" -U -Q - "$IPREFIX$PREFIX$SUFFIX$ISUFFIX"

fi

return ret
